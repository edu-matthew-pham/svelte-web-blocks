      "SUBMIT_TEXT"
    ]
  },
  "web_form_field": {
    "type": "object",
    "title": "Form Field",
    "description": "An input field within a form",
    "properties": {
      "ID": {
        "type": "string",
        "description": "Field identifier"
      },
      "CLASS": {
        "type": "string",
        "description": "CSS classes to apply"
      },
      "LABEL": {
        "type": "string",
        "description": "Field label text",
        "default": "Email"
      },
      "TYPE": {
        "type": "string",
        "description": "Type of input field",
        "enum": [
          "text",
          "email",
          "number",
          "tel",
          "textarea",
          "checkbox",
          "radio",
          "select",
          "date",
          "time"
        ],
        "default": "text"
      },
      "REQUIRED": {
        "type": "string",
